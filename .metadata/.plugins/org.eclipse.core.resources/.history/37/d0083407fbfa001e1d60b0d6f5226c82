class Outer{
	int a=100;
	int b=200;
	
	void displayOuter() {
		System.out.println(a);
		System.out.println(b);
		}
	class Inner{
		int c=300;
		int d=400;
		
		void displayInner() {
			System.out.println(c);
			System.out.println(d);
		}
	}
}

public class FirstDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
